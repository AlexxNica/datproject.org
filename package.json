{
  "name": "publicbits",
  "version": "2.0.2",
  "description": "a place to pubilsh and collaborate your datasets.",
  "main": "index.js",
  "bin": {
    "publicbits": "index.js"
  },
  "scripts": {
    "start": "npm run watch & npm run watch-css & npm run server",
    "server": "nodemon index.js",
    "test": "tape test/* | tap-spec",
    "watch": "watchify -t ractivate browser/index.js -o static/js/bundle.js",
    "watch-css": "autoless static/css",
    "build": "autoless --no-watch static/css && browserify -t ractivate browser/index.js -o static/js/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/karissa/publicbits.git"
  },
  "keywords": [
    "data",
    "datasets",
    "publish",
    "registry",
    "publicbits",
    "dat",
    "web"
  ],
  "author": "karissa",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/karissa/publicbits/issues"
  },
  "homepage": "http://dat-data.com",
  "dependencies": {
    "auth0-lock": "^8.0.4",
    "crypto": "0.0.3",
    "debug": "^2.0.0",
    "dom": "0.0.3",
    "enter-means-submit": "^1.0.1",
    "extend": "^2.0.0",
    "format-data": "^2.1.2",
    "from2": "^2.1.0",
    "hat": "0.0.3",
    "http-hash-router": "^1.1.0",
    "hyperquest": "^1.2.0",
    "is-url": "^1.1.0",
    "knex": "^0.9.0",
    "markdown-it": "^3.0.6",
    "minimist": "^1.2.0",
    "nodemon": "^1.2.1",
    "page": "^1.4.1",
    "publicbits-js": "^3.0.0",
    "pump": "^1.0.0",
    "ractive-toolkit": "^1.0.2",
    "redirecter": "^0.2.3",
    "shasum": "^1.0.2",
    "sqlite3": "^3.1.1",
    "through2": "^0.6.5",
    "transport-stream": "^1.3.1",
    "xhr": "^2.0.1"
  },
  "devDependencies": {
    "autoless": "^0.1.5",
    "browserify": "^7.0.0",
    "dat": "^7.0.4",
    "got": "^5.2.0",
    "ractivate": "^0.2.0",
    "rimraf": "^2.2.8",
    "run-series": "^1.0.2",
    "tap-spec": "^1.0.1",
    "tape": "^3.0.0",
    "tape-spawn": "^1.4.0",
    "watchify": "^2.1.1",
    "brfs": "0.0.8",
    "ejsify": "0.1.0",
    "packageify": "^0.2.0"
  }
}
